  <body>

    <div class="navbar navbar-inverse navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <a class="brand" href="#">Adoptable</a>
        </div>
      </div>
    </div>

      <div class="container">
        <header>  
          <hgroup>  
            <h1><a href="/">Adoptable</a></h1>  
            <h2>Find Your New Best Friend!</h2>  
          </hgroup>  
        </header>  
        
        <div id="main">  
      <!-- Sinatra will display layout.erb file on all routes 
      content for called route inserted with yield -->    
        <% if flash[:notice] %>  
          <p class="notice"><%= flash[:notice] %>  
        <% end %>  
          
        <% if flash[:error] %>  
          <p class="error"><%= flash[:error] %>  
        <% end %>  
       <%= yield %>  
        </div>  
        
        <footer>  
          <p><small>An app for <a href="http://aimeemarieknight.com">Aimee Knight</a>.</small></p>  
        </footer>  
      </div>

